{
  "name": "tracktivity-notifications",
  "version": "1.0.0",
  "description": "Public notification and CEX listing feed for Tracktivity dApp",
  "main": "dist/update-listings.js",
  "scripts": {
    "build": "tsc",
    "update": "npm run build && node dist/update-listings.js",
    "update:world": "npm run build && node dist/update-world-currencies.js",
    "update:all": "npm run build && node dist/update-listings.js && node dist/update-world-currencies.js",
    "dev": "ts-node src/update-listings.ts",
    "dev:world": "ts-node src/update-world-currencies.ts",
    "dev:all": "ts-node src/update-listings.ts && ts-node src/update-world-currencies.ts"
  },
  "keywords": ["tracktivity", "notifications", "cex", "listings"],
  "author": "MrJAO",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/node-fetch": "^2.6.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
